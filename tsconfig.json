{
    "compilerOptions": {
        "target": "es5",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "esModuleInterop": true,
        "module": "ESNext",
        "jsx": "react-jsx",
        "moduleResolution": "Node",
        "rootDir": "src",
        "baseUrl": "src",
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "noEmit": true
    },
    "include": ["src"]
}
